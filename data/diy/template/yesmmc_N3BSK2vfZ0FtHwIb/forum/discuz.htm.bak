<script>
function tab_m(obj,name)
{
var p = obj.parentNode.getElementsByTagName("li");
var p1 = document.getElementById(name).getElementsByTagName("DT");
for(i=0;i<p1.length;i++)
{
if(obj==p[i])
{
p[i].className = "active";
p1[i].className = "dis";
}
else
{
p[i].className = "#";
p1[i].className = "undis";
}
}
}

</script>

